    <script>
        function WebForm_OnSubmit() {
            var retValue = true;
            if (ValidatorOnSubmit && !ValidatorOnSubmit()) {
                $('.validationFailed').attr('class', 'validationSuccess');
                var itemsarray = [];
                for (var i = 0; i < Page_Validators.length; i++) {
                    var vc = Page_Validators[i];
                    var $ctv = $('#' + vc.controltovalidate);
                    if (!vc.isvalid) {
                        itemsarray.push($ctv);
                        retValue = false;
                    }

                }

                for (var index = 0; index < itemsarray.length; index++) {
                    itemsarray[index].parent().attr('class', 'validationFailed');
                }
            }
            return retValue;
        }</script>
    <script type="text/javascript">

        String.Format = function () {
            var s = arguments[0];
            for (var i = 0; i < arguments.length - 1; i++) {
                var reg = new RegExp("\\{" + i + "\\}", "gm");
                s = s.replace(reg, arguments[i + 1]);
            }
            return s;
        }

        var dialogConfirmed = false;
        function ConfirmDialog(obj, title, dialogText) {
            if (!dialogConfirmed) {
                $('body').append(String.Format("<div id='dialog' title='{0}'><p>{1}</p></div>",
                    title, dialogText));

                $('#dialog').dialog
                ({
                    height: 110,
                    modal: true,
                    resizable: false,
                    draggable: false,
                    close: function (event, ui) { $('body').find('#dialog').remove(); },
                    buttons:
                    {
                        'Yes': function () {
                            $(this).dialog('close');
                            dialogConfirmed = true;
                            if (obj) obj.click();
                        },
                        'No': function () {
                            $(this).dialog('close');
                        }
                    }
                });
            }

            return dialogConfirmed;
        }

 
    </script>
